    <div id="container">
        <div class="navLines" id="navLines">
            <div class="all-logo">
                <a id="logo" href="/"><img class="logo-image" src="../blackLogo.png"></img></a>
                <h1 class="logo">LABOROSO</h1>
            </div>
            <div id="ModalNalComponent">
                <ModalNav />
            </div>
            <div id="NavigationComponent">
                <Navigation />
            </div>
        </div>
    </div>
    
    
</header>

<style>

    @media screen and (max-width: 980px) {
        #NavigationComponent{
            display: none;
        }

        #ModalNalComponent{
            
        }
        
    }
    

.benefit {
    display: inline-block;
    margin: 1rem;
